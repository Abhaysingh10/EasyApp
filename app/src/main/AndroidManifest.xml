<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.easy">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/poster"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
        <activity android:name=".profileSetting" android:theme="@style/categories"></activity>
        <activity android:name=".profileRegister" />
        <activity
            android:name=".categories"
            android:theme="@style/categories" />
        <activity android:name=".Questionnaire" android:theme="@style/categories"/>
        <activity android:name=".loginActivity" />
        <activity android:name=".afterLogin" />
        <activity android:name=".registerActivity" />
        <activity
            android:name=".splashscreen"
            android:noHistory="true"
            android:theme="@style/s">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>